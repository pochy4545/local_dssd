<p id="notice"><%= notice %></p>

<h1>Tipo Participantes</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_participantes.each do |tipo_participante| %>
      <tr>
        <td><%= tipo_participante.tipo %></td>
        <td><%= link_to 'Show', tipo_participante %></td>
        <td><%= link_to 'Edit', edit_tipo_participante_path(tipo_participante) %></td>
        <td><%= link_to 'Destroy', tipo_participante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Participante', new_tipo_participante_path %>
