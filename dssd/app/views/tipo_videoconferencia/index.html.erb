<p id="notice"><%= notice %></p>

<h1>Tipo Videoconferencia</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_videoconferencia.each do |tipo_videoconferencium| %>
      <tr>
        <td><%= tipo_videoconferencium.tipo %></td>
        <td><%= link_to 'Show', tipo_videoconferencium %></td>
